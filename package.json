{
  "name": "@kalos/contract",
  "version": "0.1.5",
  "description": "Shared contract library for Kalos projects - types, Supabase client factory, RPC wrappers, and public queries",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "lint": "echo 'Linting not configured'",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run build && npm run typecheck",
    "db:login": "npx supabase login",
    "db:projects": "npx supabase projects list",
    "db:link": "npx supabase link --project-ref tkioedsebdxqblgcctxv",
    "db:migrations:list": "npx supabase migration list > supabase/_remote/migration-list.txt",
    "db:canonical:select": "node scripts/select-canonical-migrations.mjs",
    "db:diff": "npx supabase db diff -f",
    "db:push": "npx supabase db push",
    "db:pull": "npx supabase db pull",
    "db:start": "npx supabase start",
    "db:stop": "npx supabase stop",
    "db:import:legacy": "node scripts/import-legacy-supabase.mjs",
    "db:unify": "node scripts/unify-canonical-files.mjs",
    "db:init:migrations": "node scripts/init-migrations-from-remote.mjs",
    "db:dump:schema": "node scripts/dump-remote-schema.mjs",
    "db:fill:migrations": "node scripts/fill-migrations-from-dump.mjs",
    "db:repair:history": "node scripts/repair-migration-history.mjs",
    "verify": "npm run typecheck && npm run build",
    "verify:migrations": "node scripts/verify-migrations.mjs",
    "verify:subscriptions": "node scripts/verify-subscriptions-bookings-entries.mjs"
  },
  "keywords": [
    "supabase",
    "kalos",
    "contract",
    "types"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0"
  },
  "devDependencies": {
    "pg": "^8.16.3",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@supabase/supabase-js": "^2.39.0"
  },
  "peerDependenciesMeta": {
    "@supabase/supabase-js": {
      "optional": false
    }
  }
}
